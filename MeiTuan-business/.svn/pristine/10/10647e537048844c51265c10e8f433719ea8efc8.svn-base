
@charset "utf-8";
#footer{
	margin:0 auto;
	text-align:center;
	position:absolute;
	bottom:0;
}
#expand{
	margin-top:30px;
	margin-left:180px;
	margin-right:30px;
	float: left;
}
#contact{
	width: 280px;
	height:110px;
	margin-top:38px;
	border-left:1px dashed rgb(220,220,220);
	border-right:1px dashed rgb(220,220,220);
	float: left;
}
#QR_Code{
	margin-top:38px;
	margin-left:60px;
	float: left;
}
ul{
	display:inline-block;
}
li{
	margin: 8px 35px;
}
a,span{
	color:rgb(102,102,102);
	font-size:0.75em;
	letter-spacing:1px;
}
a:hover{
	color:rgb(0,0,0);
}
#text{
	height:100px;
	line-height:50px;
	display:inline-block;
}
div>span:first-child{
	font-size:1.2em;
	color:rgb(0,0,0);
	font-weight:bold;
}
